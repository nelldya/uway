function jQueryCheckFormLow(evt,$obj)
{
	var val = jQueryGetFormValue($obj);
	if( val==='' ) return false;
	var attr = String($obj.attr(jQcfConfig.attr.Low));
	if( attr.match(/^(false|undefined|)$/i) ) return false;
	if( Number(val) < Number(attr) )
	{
		jQuerySetFormValue($obj,'',false);
		return jQueryCheckFormTimeoutMessage(evt,$obj,$obj,jQcfConfig.msg.Low.replace(/%s/,attr));
	}
}
function jQueryCheckFormHigh(evt,$obj)
