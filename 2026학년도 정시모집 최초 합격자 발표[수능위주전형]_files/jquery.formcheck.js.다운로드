	if( attr.match(/^(false|undefined)$/i) ) return false;
	if( !jQcfConfig.exp.Ip.test(val) )
	{
		jQuerySetFormValue($obj,'',false);
		return jQueryCheckFormTimeoutMessage(evt,$obj,$obj,jQcfConfig.msg.Ip);
	}
}
function jQueryCheckFormPassword(evt,$obj)
{
	var val = jQueryGetFormValue($obj);
	if( val==='' ) return false;
	var attr = String($obj.attr(jQcfConfig.attr.Password));
	if( attr.match(/^(false|undefined)$/i) ) return false;
	var exp = jQcfConfig.exp.Password1;
	var type = String($obj.attr(jQcfConfig.attr.Password+'type'));
	if( type.match(/^[1-9]$/) ) exp = jQcfConfig.exp['Password'+type];
	if( !exp.test(val) )
	{
		jQuerySetFormValue($obj,'',false);
		var msg = jQcfConfig.msg.Password1;
		if( type.match(/^[1-9]$/) ) msg = jQcfConfig.msg['Password'+type];
		return jQueryCheckFormTimeoutMessage(evt,$obj,$obj,msg);
	}
}
function jQueryCheckFormEmail(evt,$obj)
