	if( attr.match(/^(false|undefined)$/i) ) return false;
	if( !jQcfConfig.exp.Email.test(val) )
	{
		jQuerySetFormValue($obj,'',false);
		return jQueryCheckFormTimeoutMessage(evt,$obj,$obj,jQcfConfig.msg.Email);
	}
}

function jQueryInitFormResets(evt,$obj)
{
	var val = jQueryGetFormValue($obj);
	if( val==='' ) return false;
	var attr = String($obj.attr(jQcfConfig.attr.Resets));
	if( attr.match(/^(false|undefined|)$/i) ) return false;
	if( String($obj.prop('type')).toUpperCase().match(/^(RADIO|CHECKBOX)$/) )
	{
		return $obj.attr(jQcfConfig.attr.Resets+'init','').trigger('change');
	}
	return $obj.attr(jQcfConfig.attr.Resets+'init',val);
}
function jQueryCheckFormResets(evt,$obj)
{
	if( evt.type=='click' ) return false;
	var attr = String($obj.attr(jQcfConfig.attr.Resets));
	if( attr.match(/^(false|undefined|)$/i) ) return false;
	var init = String($obj.attr(jQcfConfig.attr.Resets+'init'));
	$obj.attr(jQcfConfig.attr.Resets+'init','');
	var val = jQueryGetFormValue($obj);
	console.log(init);
	console.log(val);
	if( init===val ) return false;
	attr = attr.split(jQcfConfig.glue.Object);
	for( var i = 0; i < attr.length; i++ )
	{
		attr[i] = String(attr[i]).replace(/^\s+|\s+$/g,'');
		switch( attr[i].substr(0,1) )
		{
			case '#':
				if( jQuery(attr[i]).length > 1 ) return console.error('지정한 id의 필드가 한개가 아닙니다.'+attr[i]);
				if( jQueryGetFormValue(jQuery(attr[i]))!=='' ) jQuerySetFormValue(jQuery(attr[i]),'');
				break;
			case '.':
				jQuery(attr[i]).each(function(){ if( jQueryGetFormValue(jQuery(this))!=='' ) jQuerySetFormValue(jQuery(this),''); });
				break;
			default:
				if( attr[i].substr(-1,1)==')' )
				{
					try {
						eval(attr[i]);
					} catch (error) {
					}
				}
				else jQuery('[name="'+attr[i]+'"]').each(function(){ if( jQueryGetFormValue(jQuery(this))!=='' ) jQuerySetFormValue(jQuery(this),''); });
				break;
		}
	}
}
function jQueryCheckFormNeeds(evt,$obj)
{
	jQuery('.jq-reset').removeClass('jq-reset');
	// if( jQueryGetFormValue($obj)==='' ) return false;
	var attr = String($obj.attr(jQcfConfig.attr.Needs));
	if( attr.match(/^(false|undefined|)$/i) ) return false;
	attr = attr.split(jQcfConfig.glue.Object);
	for( var i = 0; i < attr.length; i++ )
	{
		attr[i] = String(attr[i]).replace(/^\s+|\s+$/g,'');
		if( attr[i].indexOf('??') > -1 )
		{
			var attri = attr[i].split(jQcfConfig.glue.OrObject);
			var or = false;
			for( var j = 0; j < attri.length; j++ )
			{
				switch( String(attri[j]).substr(0,1) )
				{
					case '#':
						if( jQuery(attri[j]).length > 1 ) return console.error('지정한 id의 필드가 한개가 아닙니다.'+attri[j]);
						if( jQueryGetFormValue(jQuery(attri[j]))!=='' ) or = true;
						break;
					case '.':
						jQuery(attri[j]).each(function()
						{
							if( jQueryGetFormValue(jQuery(this))!=='' )
							{
								or = true;
								return false;
							}
						});
						break;
					default:
						if( attri[j].substr(-1,1)==')' )
						{
							try {
								eval('var res = '+attri[j]+';');
							} catch (error) {
								var res = false;
							}
							if( res===true ) or = true;
						}
						else
						{
							jQuery('[name="'+attri[j]+'"]').each(function()
							{
								if( jQueryGetFormValue(jQuery(this))!=='' )
								{
									or = true;
									return false;
								}
							});
						}
						break;
				}
				if( or===true ) break;
			}
			if( or===false )
			{
				var res = true;
				for( var j = 0; j < attri.length; j++ )
				{
					res = jQueryCheckFormNeedsCheck(evt,$obj,attri[j],jQcfConfig.msg.NeedsOr);
					res = false;
					// if( res===false ) break;
				}
				if( res===false ) break;
			}
		}
		else
		{
			if( jQueryCheckFormNeedsCheck(evt,$obj,attr[i])===false ) break;
		}
	}
}
var jQueryCheckFormOnMessage = false;
function jQueryCheckFormNeedsCheck(evt,$obj,attr,msg)
{
	msg = msg || jQcfConfig.msg.Needs;

	switch( String(attr).substr(0,1) )
	{
		case '#':
			if( jQuery(attr).length > 1 ) return console.error('지정한 id의 필드가 한개가 아닙니다.'+attr);
			if( jQueryGetFormValue(jQuery(attr))==='' )
			{
				jQueryGlowObject(jQuery(attr));
				jQuerySetFormValue($obj,'',false);
				return jQueryCheckFormTimeoutMessage(evt,$obj,jQuery(attr),msg);
			}
			break;
		case '.':
			var res = true;
			jQuery(attr).each(function()
			{
				if( jQueryGetFormValue(jQuery(this))==='' )
				{
					jQueryGlowObject(jQuery(this));
					jQuerySetFormValue($obj,'',false);
					res = false;
					return jQueryCheckFormTimeoutMessage(evt,$obj,jQuery(this),msg);
				}
			});
			if( res===false ) return false;
			break;
		default:
			if( attr.substr(-1,1)==')' )
			{
				try {
					eval('var res = '+attr+';');
				} catch (error) {
					var res = true;
				}
				if( res!==true )
				{
					evt.preventDefault();
					evt.stopImmediatePropagation();
					alert(res);
					return false;
				}
			}
			else
			{
				var res = true;
				jQuery('[name="'+attr+'"]').each(function()
				{
					if( jQueryGetFormValue(jQuery(this))==='' )
					{
						jQueryGlowObject(jQuery(this));
						jQuerySetFormValue($obj,'',false);
						res = false;
						return jQueryCheckFormTimeoutMessage(evt,$obj,jQuery(this),msg);
					}
				});
				if( res===false ) return false;
			}
			break;
	}
}

function jQueryCheckFormTimeoutMessage(evt,$origin,$obj,msg)
{
	if( jQueryCheckFormOnMessage ) return false;

	evt.preventDefault();
	evt.stopImmediatePropagation();
	if( $origin==$obj )
	{
		var msg1 = String($origin.attr(jQcfConfig.attr.Msg));
		if( !msg1.match(/^(false|undefined)$/i) ) msg = msg1;
	}
	if( msg===jQcfConfig.msg.NeedsOr )
	{
		jQueryCheckFormOnMessage = true;
		alert(msg,function()
		{
			window.jQueryCheckFormOnMessage = false;
		});
	}
	else if( msg!=='' )
	{
		$obj.after('<div class="jq-need jq-need-msg">'+msg+'</div>').trigger('focus');
		window.setTimeout(function()
		{
			$obj.siblings('.jq-need-msg:last').remove();
		},3500);
	}
	var offsetTop =Number($obj.offset().top)-70;
	var y = Number(window.scrollY);
	var ih = Number(window.innerHeight) + y;
	if( y > offsetTop || ih < offsetTop ) window.scrollTo(0,offsetTop);
	return false;
}