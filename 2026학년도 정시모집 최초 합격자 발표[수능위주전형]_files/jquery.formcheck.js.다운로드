	if( attr.match(/^(false|undefined|)$/i) ) return false;
	if( Number(val) > Number(attr) )
	{
		jQuerySetFormValue($obj,'',false);
		return jQueryCheckFormTimeoutMessage(evt,$obj,$obj,jQcfConfig.msg.High.replace(/%s/,attr));
	}
}
function jQueryCheckFormIp(evt,$obj)
