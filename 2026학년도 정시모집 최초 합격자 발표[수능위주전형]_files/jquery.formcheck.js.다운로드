		Resets			: 'border: solid .2rem red !important;',
	},
	setvalTime: 500,
};
jQuery(function()
{
	jQuery('['+jQcfConfig.attr.Needs+']').on('click',function(evt){ jQueryCheckFormNeeds(evt,jQuery(this)); })
		// .on('focus',function(evt){ jQueryCheckFormNeeds(evt,jQuery(this)); })
		.on('change',function(evt){ jQueryCheckFormNeeds(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.Resets+']').on('click',function(evt){ jQueryInitFormResets(evt,jQuery(this)); })
		// .on('focus',function(evt){ jQueryInitFormResets(evt,jQuery(this)); })
		.on('change',function(evt){ jQueryCheckFormResets(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.Email+']').on('change',function(evt){ jQueryCheckFormEmail(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.Password+']').on('change',function(evt){ jQueryCheckFormPassword(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.Ip+']').on('change',function(evt){ jQueryCheckFormIp(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.Int+']').on('change',function(evt){ jQueryCheckFormInt(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.Float+']').on('change',function(evt){ jQueryCheckFormFloat(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.Plus+']').on('change',function(evt){ jQueryCheckFormPlus(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.Low+']').on('change',function(evt){ jQueryCheckFormLow(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.High+']').on('change',function(evt){ jQueryCheckFormHigh(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.Number+'],['+jQcfConfig.attr.Korean+'],['+jQcfConfig.attr.KoreanJamo+'],['+jQcfConfig.attr.English+'],['+jQcfConfig.attr.Chainese+'],['+jQcfConfig.attr.Japanese+'],['+jQcfConfig.attr.Special+'],['+jQcfConfig.attr.Space+']').on('change',function(evt){ jQueryCheckFormText(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.NotNumber+'],['+jQcfConfig.attr.NotKorean+'],['+jQcfConfig.attr.NotKoreanJamo+'],['+jQcfConfig.attr.NotEnglish+'],['+jQcfConfig.attr.NotChainese+'],['+jQcfConfig.attr.NotJapanese+'],['+jQcfConfig.attr.NotSpecial+'],['+jQcfConfig.attr.NotSpace+']').on('change',function(evt){ jQueryCheckFormNotText(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.Exp+']').on('change',function(evt){ jQueryCheckFormExp(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.LengthMin+']').on('change',function(evt){ jQueryCheckFormLengthMin(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.LengthMax+']').on('change',function(evt){ jQueryCheckFormLengthMax(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.Upper+']').on('change',function(evt){ jQueryCheckFormLengthUpper(evt,jQuery(this)); });
	jQuery('['+jQcfConfig.attr.Lower+']').on('change',function(evt){ jQueryCheckFormLengthLower(evt,jQuery(this)); });
	jQuery('head').append('<style>.jq-need-msg{ '+jQcfConfig.css.Needs+' }</style>');
	jQuery('head').append('<style>.jq-reset{ '+jQcfConfig.css.Resets+' }</style>');
});

function jQueryGetFormValue($obj)
{
	if( $obj.length < 1 ) return false;
	var tag = String($obj.prop('tagName')).toUpperCase();
	if( tag==='INPUT' )
	{
		switch( String($obj.prop('type')).toUpperCase() )
		{
			case 'HIDDEN':	case 'TEXT':	case 'PASSWORD':	case 'EMAIL':			case 'NUMBER':
			case 'TEL':		case 'DATE':	case 'MONTH':		case 'DATETIME-LOCAL':	case 'DATETIME':
			case 'COLOR':	case 'FILE':	case 'RANGE':		case 'SEARCH':			case 'TIME':
			case 'URL':		case 'WEEK':
				var val = $obj.val().replace(/^\s+|\s+$/g,'');
				$obj.val(val);
				return val;
			case 'RADIO':
				var $checked = jQuery('[name="'+$obj.prop('name')+'"]:checked');
				return $checked.length? $checked.val():'';
			case 'CHECKBOX':
				var val = [];
				var $checked = jQuery('[name="'+$obj.prop('name')+'"]:checked');
				if( $checked.length ) $checked.each(function(){ val.push(jQuery(this).val()); });
				return val.join(jQcfConfig.glue.Value);
		}
	}
	else if( tag==='SELECT' )
	{
		var val = [];
		$obj.find('option:selected').each(function(){ val.push(jQuery(this).val()); });
		return val.join(jQcfConfig.glue.Value);
	}
	else if( tag==='TEXTAREA' )
	{
		return $obj.val();
	}
}
function jQueryGlowObject($obj)
{
	window.setTimeout(function(){ $obj.addClass('jq-reset'); },jQcfConfig.setvalTime*0);
	window.setTimeout(function(){ $obj.removeClass('jq-reset'); },jQcfConfig.setvalTime*1);
	window.setTimeout(function(){ $obj.addClass('jq-reset'); },jQcfConfig.setvalTime*2);
	window.setTimeout(function(){ $obj.removeClass('jq-reset'); },jQcfConfig.setvalTime*3);
	window.setTimeout(function(){ $obj.addClass('jq-reset'); },jQcfConfig.setvalTime*4);
	$obj.on('click',function()
	{
		jQuery('.jq-reset').removeClass('jq-reset');
	}).on('change',function()
	{
		jQuery('.jq-reset').removeClass('jq-reset');
	});
}
function jQuerySetFormValue($obj,val,glow)
{
	if( $obj.length < 1 ) return false;
	var tag = String($obj.prop('tagName')).toUpperCase();
	var type = String($obj.prop('type')).toUpperCase();
	if( tag==='A' || tag==='SPAN' || tag==='BUTON' || type==='BUTTON' ) return false;
	if( glow!==false )
	{
		jQueryGlowObject($obj);
	}
	if( tag==='INPUT' )
	{
		switch( type )
		{
			case 'HIDDEN':	case 'TEXT':	case 'PASSWORD':	case 'EMAIL':			case 'NUMBER':
			case 'TEL':		case 'DATE':	case 'MONTH':		case 'DATETIME-LOCAL':	case 'DATETIME':
			case 'COLOR':	case 'FILE':	case 'RANGE':		case 'SEARCH':			case 'TIME':
			case 'URL':		case 'WEEK':
				$obj.val(val);
				break;
			case 'RADIO':
				jQuery('[name="'+$obj.prop('name')+'"]').each(function()
				{
					if( jQuery(this).val()===val ) jQuery(this).prop('checked',true);
					else jQuery(this).prop('checked',false);
				});
				break;
			case 'CHECKBOX':
				val = val.split(jQcfConfig.glue.Value);
				jQuery('[name="'+$obj.prop('name')+'"]').each(function()
				{
					var m = false;
					for( var i=0; i < val.length; i++ )
					{
						if( jQuery(this).val()===val[i] )
						{
							m = true;
							break;
						}
					}
					if( m ) jQuery(this).prop('checked',true);
					else jQuery(this).prop('checked',false);
				});
				break;
		}
	}
	else if( tag==='SELECT' )
	{
		val = val.split(jQcfConfig.glue.Value);
		$obj.find('option').each(function()
		{
			var m = false;
			for( var i=0; i < val.length; i++ )
			{
				if( jQuery(this).val()===val[i] )
				{
					m = true;
					break;
				}
			}
			if( m ) jQuery(this).prop('selected',true);
			else jQuery(this).prop('selected',false);
		});
	}
	else if( tag==='TEXTAREA' )
	{
		$obj.val(val);
	}
}
function jQueryGetByteFormValue(s,b,i,c)	//s만 입력
{
	// for( b=i=0; c=s.charCodeAt(i++); b+=c>>11?3:c>>7?2:1 );	//utf-8 체크
	for( b=i=0; c=s.charCodeAt(i++); b+=c>>7?2:1 );	//utf-8 체크 안하고 유니코드는 무조건 2바이트
	return b;
}

function jQueryCheckFormLengthUpper(evt,$obj)
{
	var val = jQueryGetFormValue($obj);
	if( val==='' ) return false;
	var attr = String($obj.attr(jQcfConfig.attr.Upper));
	if( attr.match(/^(false|undefined)$/i) ) return false;
	jQuerySetFormValue($obj,val.toUpperCase(),false);
}
function jQueryCheckFormLengthLower(evt,$obj)
{
	var val = jQueryGetFormValue($obj);
	if( val==='' ) return false;
	var attr = String($obj.attr(jQcfConfig.attr.Lower));
	if( attr.match(/^(false|undefined)$/i) ) return false;
	jQuerySetFormValue($obj,val.toLowerCase(),false);
}
function jQueryCheckFormLengthMin(evt,$obj)
{
	var val = jQueryGetFormValue($obj);
	if( val==='' ) return false;
	var attr = String($obj.attr(jQcfConfig.attr.LengthMin));
	if( attr.match(/^(false|undefined|)$/i) ) return false;
	var tag = String($obj.prop('tagName')).toUpperCase();
	var type = String($obj.prop('type')).toUpperCase();
	if( tag==='SELECT' || type==='CHECKBOX' || type==='RADIO' ) return false;
	var at = String($obj.attr(jQcfConfig.attr.LengthType));
	var l = val.length;
	if( at.match(/^(byte)$/i) ) l = jQueryGetByteFormValue(val);
	if( Number(l) < Number(attr) )
	{
		jQuerySetFormValue($obj,'',false);
		var msg = jQcfConfig.msg.LengthMin;
		if( tag==='SELECT' || type==='CHECKBOX' || type==='RADIO' ) msg = jQcfConfig.msg.LengthMinAr;
		else if( at.match(/^(byte)$/i) ) jQcfConfig.msg.LengthMinB;
		return jQueryCheckFormTimeoutMessage(evt,$obj,$obj,msg.replace(/%s/,attr));
	}
}
function jQueryCheckFormLengthMax(evt,$obj)
{
	var val = jQueryGetFormValue($obj);
	if( val==='' ) return false;
	var attr = String($obj.attr(jQcfConfig.attr.LengthMax));
	if( attr.match(/^(false|undefined|)$/i) ) return false;
	var tag = String($obj.prop('tagName')).toUpperCase();
	var type = String($obj.prop('type')).toUpperCase();
	var at = String($obj.attr(jQcfConfig.attr.LengthType));
	var l = val.length;
	if( tag==='SELECT' || type==='CHECKBOX' || type==='RADIO' ) l = val.split(jQcfConfig.glue.Value).length;
	else if( at.match(/^(byte)$/i) ) l = jQueryGetByteFormValue(val);
	if( Number(l) > Number(attr) )
	{
		jQuerySetFormValue($obj,'',false);
		var msg = jQcfConfig.msg.LengthMax;
		if( tag==='SELECT' || type==='CHECKBOX' || type==='RADIO' ) msg = jQcfConfig.msg.LengthMaxAr;
		else if( at.match(/^(byte)$/i) ) jQcfConfig.msg.LengthMaxB;
		return jQueryCheckFormTimeoutMessage(evt,$obj,$obj,msg.replace(/%s/,attr));
	}
}
function jQueryCheckFormText(evt,$obj)
{
	var val = jQueryGetFormValue($obj);
	if( val==='' ) return false;
	var attr = {
		num: String($obj.attr(jQcfConfig.attr.Number)),
		kor: String($obj.attr(jQcfConfig.attr.Korean)),
		korjamo: String($obj.attr(jQcfConfig.attr.KoreanJamo)),
		eng: String($obj.attr(jQcfConfig.attr.English)),
		chn: String($obj.attr(jQcfConfig.attr.Chainese)),
		jpn: String($obj.attr(jQcfConfig.attr.Japanese)),
		spc: String($obj.attr(jQcfConfig.attr.Special)),
		s: String($obj.attr(jQcfConfig.attr.Space)),
	};
	var expstr = [];
	if( !attr.num.match(/^(false|undefined)$/i) ) expstr.push(jQcfConfig.exp.Number);
	if( !attr.kor.match(/^(false|undefined)$/i) ) expstr.push(jQcfConfig.exp.Korean);
	if( !attr.korjamo.match(/^(false|undefined)$/i) ) expstr.push(jQcfConfig.exp.KoreanJamo);
	if( !attr.eng.match(/^(false|undefined)$/i) ) expstr.push(jQcfConfig.exp.English);
	if( !attr.chn.match(/^(false|undefined)$/i) ) expstr.push(jQcfConfig.exp.Chainese);
	if( !attr.jpn.match(/^(false|undefined)$/i) ) expstr.push(jQcfConfig.exp.Japanese);
	if( !attr.spc.match(/^(false|undefined)$/i) ) expstr.push(attr.spc.replace('-','\\-').replace('[','\\[').replace(']','\\]'));
	if( !attr.s.match(/^(false|undefined)$/i) ) expstr.push(jQcfConfig.exp.Space);
	expstr = expstr.join('');
	var exp = new RegExp('^['+expstr+']+$');
	if( !exp.test(val) )
	{
		jQuerySetFormValue($obj,'',false);
		var msg = [];
		if( !attr.num.match(/^(false|undefined)$/i) ) msg.push('숫자');
		if( !attr.kor.match(/^(false|undefined)$/i) ) msg.push('한글');
		if( !attr.korjamo.match(/^(false|undefined)$/i) ) msg.push('한글');
		if( !attr.eng.match(/^(false|undefined)$/i) ) msg.push('영어');
		if( !attr.chn.match(/^(false|undefined)$/i) ) msg.push('중국어');
		if( !attr.jpn.match(/^(false|undefined)$/i) ) msg.push('일본어');
		if( !attr.spc.match(/^(false|undefined)$/i) ) msg.push('특수기호('+attr.spc+')');
		if( !attr.s.match(/^(false|undefined)$/i) ) msg.push('공백문자');
		msg = msg.join(',')+'만 입력할 수 있습니다.';
		return jQueryCheckFormTimeoutMessage(evt,$obj,$obj,msg);
	}
}
function jQueryCheckFormNotText(evt,$obj)
{
	var val = jQueryGetFormValue($obj);
	if( val==='' ) return false;
	var attr = {
		num: String($obj.attr(jQcfConfig.attr.NotNumber)),
		kor: String($obj.attr(jQcfConfig.attr.NotKorean)),
		korjamo: String($obj.attr(jQcfConfig.attr.KoreanJamo)),
		eng: String($obj.attr(jQcfConfig.attr.NotEnglish)),
		chn: String($obj.attr(jQcfConfig.attr.NotChainese)),
		jpn: String($obj.attr(jQcfConfig.attr.NotJapanese)),
		spc: String($obj.attr(jQcfConfig.attr.NotSpecial)),
		s: String($obj.attr(jQcfConfig.attr.NotSpace)),
	};
	var expstr = [];
	if( !attr.num.match(/^(false|undefined)$/i) ) expstr.push(jQcfConfig.exp.Number);
	if( !attr.kor.match(/^(false|undefined)$/i) ) expstr.push(jQcfConfig.exp.Korean);
	if( !attr.korjamo.match(/^(false|undefined)$/i) ) expstr.push(jQcfConfig.exp.KoreanJamo);
	if( !attr.eng.match(/^(false|undefined)$/i) ) expstr.push(jQcfConfig.exp.English);
	if( !attr.chn.match(/^(false|undefined)$/i) ) expstr.push(jQcfConfig.exp.Chainese);
	if( !attr.jpn.match(/^(false|undefined)$/i) ) expstr.push(jQcfConfig.exp.Japanese);
	if( !attr.spc.match(/^(false|undefined)$/i) ) expstr.push(attr.spc.replace('-','\\-').replace('[','\\[').replace(']','\\]'));
	if( !attr.s.match(/^(false|undefined)$/i) ) expstr.push(jQcfConfig.exp.Space);
	expstr = expstr.join('');
	var exp = new RegExp('['+expstr+']');
	if( exp.test(val) )
	{
		jQuerySetFormValue($obj,'',false);
		var msg = [];
		if( !attr.num.match(/^(false|undefined)$/i) ) msg.push('숫자');
		if( !attr.kor.match(/^(false|undefined)$/i) ) msg.push('한글');
		if( !attr.korjamo.match(/^(false|undefined)$/i) ) msg.push('한글');
		if( !attr.eng.match(/^(false|undefined)$/i) ) msg.push('영어');
		if( !attr.chn.match(/^(false|undefined)$/i) ) msg.push('중국어');
		if( !attr.jpn.match(/^(false|undefined)$/i) ) msg.push('일본어');
		if( !attr.spc.match(/^(false|undefined)$/i) ) msg.push('특수기호('+attr.spc+')');
		if( !attr.s.match(/^(false|undefined)$/i) ) msg.push('공백문자');
		msg = msg.join(',')+'를 입력할 수 없습니다.';
		return jQueryCheckFormTimeoutMessage(evt,$obj,$obj,msg);
	}
}
function jQueryCheckFormExp(evt,$obj)
{
	var val = jQueryGetFormValue($obj);
	if( val==='' ) return false;
	var attr = String($obj.attr(jQcfConfig.attr.Exp));
	if( attr.match(/^(false|undefined)$/i) ) return false;
	var exp = new RegExp(attr);
	if( !exp.test(val) )
	{
		jQuerySetFormValue($obj,'',false);
		return jQueryCheckFormTimeoutMessage(evt,$obj,$obj,jQcfConfig.msg.Exp);
	}
}
function jQueryCheckFormFloat(evt,$obj)
{
	var val = jQueryGetFormValue($obj);
	if( val==='' ) return false;
	var attr = String($obj.attr(jQcfConfig.attr.Float));
	if( attr.match(/^(false|undefined)$/i) ) return false;
	if( !jQcfConfig.exp.Float.test(val) )
	{
		jQuerySetFormValue($obj,'',false);
		return jQueryCheckFormTimeoutMessage(evt,$obj,$obj,jQcfConfig.msg.Float);
	}
}
function jQueryCheckFormPlus(evt,$obj)
