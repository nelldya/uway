			// $tempwin.parent().find('.ui-icon-closethick').css('background-color',$bgcolor);
			// $tempwin.parent().find('.ui-widget-header > a.ui-dialog-titlebar-close').removeClass('ui-dialog-titlebar-close ui-corner-all').mouseover(function(){$(this).css('border','none');});
